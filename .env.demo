# Quickbase Configuration
QB_REALM=your-realm-here
QB_USER_TOKEN=your-quickbase-user-token
QB_APP_ID=your-app-id

# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket-name
BEDROCK_AGENT_ID=your-bedrock-agent-id
BEDROCK_AGENT_ALIAS_ID=your-agent-alias-id

# API Gateway
API_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com/prod

# Slack Configuration (Optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_CHANNEL=#your-channel

# Demo Mode (set to true to test frontend without AWS)
DEMO_MODE=false

# Demo API Key (for testing without authentication)
DEMO_KEY=demo-key-12345
